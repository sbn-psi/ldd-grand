  <DD_Rule>
    <local_identifier>rule_grand_parameters</local_identifier>
    <rule_context>grand:GRaND_Parameters</rule_context>
    <rule_assign>name="local_id" value="pds:Local_Internal_Reference/pds:local_identifier_reference"</rule_assign>    
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>//(pds:Table_Character | pds:Table_Delimited | pds:Table_Binary)[pds:local_identifier = $local_id]</rule_test>
      <rule_message>In the grand:GRaND_Parameters class, the local_identifier_reference (&lt;sch:value-of select='$local_id'/&gt;) must reference a table.</rule_message>
      <rule_description>In the grand:GRaND_Parameters class, the local_identifier_reference must reference a table.</rule_description>
    </DD_Rule_Statement>
  </DD_Rule>

  <DD_Rule>
    <local_identifier>grand_parameters_ref</local_identifier>
    <rule_context>grand:GRaND_Parameters/pds:Local_Internal_Reference</rule_context>
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>pds:local_reference_type = ('grand_parameters_to_product')</rule_test>
      <rule_message>In the grand:GRaND_Parameters class, the attribute pds:reference_type must be equal to 'grand_parameters_to_product'.</rule_message>
    </DD_Rule_Statement>
  </DD_Rule>

  <DD_Rule>
    <local_identifier>rules_tel_readout_list</local_identifier>
    <rule_context>grand:Tel_Readout_List</rule_context>
    <rule_assign>name="local_id" value="../pds:Local_Internal_Reference/pds:local_identifier_reference"</rule_assign>
    <rule_assign>name="record_count" value="//(pds:Table_Character | pds:Table_Delimited | pds:Table_Binary)[pds:local_identifier=$local_id]/pds:records"</rule_assign>
    <rule_assign>name="count" value="count(grand:Tel_Readout)"</rule_assign>

    <DD_Rule_Statement>
        <rule_type>Assert</rule_type>
        <rule_test>grand:Tel_Readout[grand:first_record=1]</rule_test>
        <rule_message>The first Tel_Readout must start with '1'</rule_message>
    </DD_Rule_Statement>

    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>grand:Tel_Readout[position()=$count]/grand:last_record=$record_count</rule_test>
      <rule_message>One Tel_Readout must end with the value of the record count (&lt;sch:value-of select='$record_count'/&gt;)</rule_message>
    </DD_Rule_Statement>

  </DD_Rule>

  <DD_Rule>
    <local_identifier>rules_tel_readout</local_identifier>
    <rule_context>grand:Tel_Readout</rule_context>
    <rule_assign>name="last_record" value="grand:last_record"</rule_assign>
    <rule_assign>name="position" value="position()"</rule_assign>    
    <rule_assign>name="count" value="count(../grand:Tel_Readout)"</rule_assign>    
    <rule_assign>name="local_id" value="../../pds:Local_Internal_Reference/pds:local_identifier_reference"</rule_assign>
    <rule_assign>name="record_count" value="number(//*[pds:local_identifier=$local_id]/pds:records)"</rule_assign>
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>grand:last_record >= grand:first_record</rule_test>
      <rule_message>In the grand:Tel_Readout class, the index of the last record must be greater than or equal to the index of the first record.</rule_message>
    </DD_Rule_Statement>
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>($position=$count or ../grand:Tel_Readout[position()=$position+1]/grand:first_record=$last_record+1)</rule_test>
      <rule_message>Each Tel_Readout must be followed by a new Tel_Readout that starts where the previous one ended, except for the last record.</rule_message>
    </DD_Rule_Statement>
  </DD_Rule>


  <DD_Rule>
    <local_identifier>rules_tel_soh_list</local_identifier>
    <rule_context>grand:Tel_SOH_List</rule_context>
    <rule_assign>name="local_id" value="../pds:Local_Internal_Reference/pds:local_identifier_reference"</rule_assign>
    <rule_assign>name="record_count" value="number(//*[pds:local_identifier=$local_id]/pds:records)"</rule_assign>


    <DD_Rule_Statement>
        <rule_type>Assert</rule_type>
        <rule_test>grand:Tel_SOH[grand:first_record=1]</rule_test>
        <rule_message>The first Tel_SOH must start with '1'</rule_message>
    </DD_Rule_Statement>

    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>grand:Tel_SOH[grand:last_record=$record_count]</rule_test>
      <rule_message>One Tel_SOH must end with the value of the record count</rule_message>
    </DD_Rule_Statement>
  </DD_Rule>

  <DD_Rule>
    <local_identifier>rules_tel_soh</local_identifier>
    <rule_context>grand:Tel_SOH</rule_context>
    <rule_assign>name="last_record" value="grand:last_record"</rule_assign>
    <rule_assign>name="position" value="position()"</rule_assign>    
    <rule_assign>name="count" value="count(../grand:Tel_SOH)"</rule_assign>    
    <rule_assign>name="local_id" value="../../pds:Local_Internal_Reference/pds:local_identifier_reference"</rule_assign>
    <rule_assign>name="record_count" value="number(//*[pds:local_identifier=$local_id]/pds:records)"</rule_assign>
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>grand:last_record >= grand:first_record</rule_test>
      <rule_message>In the grand:Tel_SOH class, the index of the last record must be greater than or equal to the index of the first record.</rule_message>
    </DD_Rule_Statement>
    <DD_Rule_Statement>
      <rule_type>Assert</rule_type>
      <rule_test>($position=$count or ../grand:Tel_SOH[position()=$position+1]/grand:first_record=$last_record+1)</rule_test>
      <rule_message>Each Tel_SOH must be followed by a new Tel_SOH that starts where the previous one ended, except for the last record.</rule_message>
    </DD_Rule_Statement>
  </DD_Rule>

</Ingest_LDD>
